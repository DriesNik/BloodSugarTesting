@model IEnumerable<BllodSugarTester.Models.BloodSugars>
@using Microsoft.AspNet.Identity
@{
    ViewBag.Title = "DisplayNumbers";
}

<h2>DisplayNumbers</h2>
<div id='displayed'>

    @foreach (var info in Model)
    {
        if (info.UserId == User.Identity.GetUserId())
        {
            
    
            <p>@info.BloodSugar, at @info.Time, on @info.Date</p>
       
            }
        }
    

</div>
<div>
    @Html.ActionLink("View This Month's Number", "DisplayNumbersMonth", "Home")
    
</div>
<script >
    function month() {


        $('#displayed').append(
            "<tr>" +
            "<td>" + tested()+ "</td>"

            + "</tr>"
            );

    }


    function tested() {
       
           @foreach (var info in Model)
           {
               
               if (info.UserId == User.Identity.GetUserId())
               {


                   @info.BloodSugar.ToString();
                   
                   @*@info.Time.ToString();*@
                  @info.Date.ToString();
                       

               }

           }
           
    }
</script>